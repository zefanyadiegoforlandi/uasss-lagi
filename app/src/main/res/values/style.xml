<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CustomAlertDialog" parent="ThemeOverlay.MaterialComponents.Dialog.Alert">
        <item name="colorPrimary">#3F51B5</item>
        <item name="colorPrimaryDark">#303F9F</item>
        <item name="colorAccent">#FF4081</item>
        <item name="android:background">@drawable/rounded_rectangle</item>
        <item name="buttonBarPositiveButtonStyle">@style/AlertDialogButtonStyleNegative</item>
        <item name="buttonBarNegativeButtonStyle">@style/AlertDialogButtonStyleLogOut</item>
    </style>

    <style name="AlertDialogButtonStyleLogOut" parent="Widget.MaterialComponents.Button.TextButton.Dialog">
        <item name="android:textColor">?attr/colorError</item>
    </style>

    <style name="AlertDialogButtonStyleNegative" parent="Widget.MaterialComponents.Button.TextButton.Dialog">
        <item name="android:textColor">?attr/colorPrimary</item>
    </style>

    <style name="RoundedExtendedFloatingActionButton" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item> <!-- Adjust the cornerSize as needed -->
    </style>

</resources>